{
  "name": "mv-auth-backend",
  "version": "1.0.0",
  "description": "This project contain services to authenticate metaverse users",
  "main": "dist/server.js",
  "scripts": {
    "start": "tsc && node dist/server.js",
    "start-dev": "npm run migrate && tsc && nodemon server.ts",
    "test": "cross-env NODE_ENV=test jest --coverage --silent --runInBand --testTimeout=10000",
    "test:watch": "jest --watch",
    "prod": "npm run build && npm run start",
    "build": "tsc -b",
    "lint": "eslint .",
    "migrate": "npx sequelize-cli db:migrate",
    "migrated": "sequelize db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wrld3d/mv-auth.git"
  },
  "keywords": [
    "Metaverse"
  ],
  "author": "Farhan Zia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wrld3d/mv-auth/issues"
  },
  "homepage": "https://github.com/wrld3d/mv-auth#readme",
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^4.0.0",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.0.0",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@metamask/eth-sig-util": "^4.0.0",
    "@types/jest": "^26.0.4",
    "@types/multer": "^1.4.7",
    "typescript": "^4.3.4",
    "axios": "^0.24.0",
    "axios-retry": "3.1.9",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "ethereumjs-util": "^7.0.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-validator": "^6.11.1",
    "jest": "^26.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.0.3",
    "pg-hstore": "^2.3.4",
    "redis": "4.0.0",
    "sequelize": "6.17.0",
    "sequelize-cli": "^6.3.0",
    "ts-jest": "^26.1.3",
    "ts-node": "10.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "helmet": "^4.6.0",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.6",
    "winston": "^3.2.1"
  },
  "devDependencies": {}
}
